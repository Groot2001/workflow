tep=\"n\"\n        >\n          <v-card\n            class=\"mb-12\"\n            color=\"grey lighten-1\"\n            height=\"200px\"\n          ><p>hello</p></v-card>\n\n          <v-btn\n            color=\"primary\"\n            @click=\"nextStep(n)\"\n          >\n            Continue\n          </v-btn>\n\n          <v-btn text>Cancel</v-btn>\n        </v-stepper-content>\n      </v-stepper-items>\n    </v-stepper>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"profile\",\n         data () {\n      return {\n        e1: 1,\n        steps: 2,\n      }\n    },\n\n    watch: {\n      steps (val) {\n        if (this.e1 > val) {\n          this.e1 = val\n        }\n      },\n    },\n\n    methods: {\n      nextStep (n) {\n        if (n === this.steps) {\n          this.e1 = 1\n        } else {\n          this.e1 = n + 1\n        }\n      },\n    },\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}